{
  "name": "@vinicius1313/eslint-config",
  "description": "Eslint configuration (includes TypeScript)",
  "version": "1.2.2",
  "license": "MIT",
  "main": ".eslintrc.js",
  "files": [
    "lib",
    "src",
    ".eslintrc.js",
    "README.md"
  ],
  "author": "Vinicius Yoshioka <71671043+viniciusyoshioka@users.noreply.github.com> (https://github.com/viniciusyoshioka)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniciusyoshioka/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/viniciusyoshioka/eslint-config/issues"
  },
  "homepage": "https://github.com/viniciusyoshioka/eslint-config",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "TypeScript",
    "TS",
    "configuration",
    "config",
    "eslint",
    "lint"
  ],
  "scripts": {
    "build:commonjs": "tsc -p ./configs/tsconfig.commonjs.json",
    "build:module": "tsc -p ./configs/tsconfig.module.json",
